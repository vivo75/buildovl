CFLAGS="${CFLAGS} -fhardened -fpic -D_LARGEFILE_SOURCE -D_FILE_OFFSET_BITS=64"
CXXFLAGS="${CXXFLAGS} -fhardened -fpic -D_LARGEFILE_SOURCE -D_FILE_OFFSET_BITS=64"

